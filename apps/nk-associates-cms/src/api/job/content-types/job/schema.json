      "minLength": 2
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 6
    },
    "responsibilities": {
      "type": "text",
      "required": true,
      "minLength": 6
    },
    "qualification": {
      "type": "string",
      "required": true,
      "minLength": 2
    },
    "positions": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "start": {
      "type": "time",
      "required": true
    },
    "end": {
      "type": "time",
      "required": true
    },
    "department": {
      "type": "relation",
