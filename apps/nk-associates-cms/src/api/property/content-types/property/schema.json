{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 40,
      "minLength": 6
    },
    "address": {
      "type": "string",
      "required": true,
      "maxLength": 40,
      "minLength": 6
    },
    "city": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "bedrooms": {
      "type": "integer",
      "min": 1
    },
    "baths": {
      "type": "integer",
      "min": 1
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "area": {
      "type": "decimal",
      "required": true,
      "min": 1
    },
    "area_type": {
      "type": "enumeration",
      "enum": [
        "Sq.Yd",
        "Sq.Ft",
        "Sq.M",
        "Marla",
        "Kanal"
      ],
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "image_thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "property_images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "property_pdf": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "property_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::property-category.property-category"
    },
    "property_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::property-type.property-type"
    },
    "completion_status": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::completion-status.completion-status"
    },
    "rent_frequency": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::rent-frequency.rent-frequency"
    },
    "property_purpose": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::property-purpose.property-purpose"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "properties"
    },
    "vr_tour": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::vr-tour.vr-tour"
    },
    "property_location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::property-location.property-location"
    },
    "property_top_picks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::property-top-pick.property-top-pick",
      "mappedBy": "properties"
    }
  }
}
